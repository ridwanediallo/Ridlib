<% if user_signed_in? %>
        <%= link_to current_user.name, edit_user_registration_path, class: "text-white font-bold hover:underline" %>
        <%= link_to "Log Out", destroy_user_session_path, method: :delete, class: "text-white font-bold hover:underline" %>
<% else %>
        <%= link_to "Sign In", new_user_session_path, class: "text-white font-bold hover:underline" %>
        <%= link_to "Sign up", new_user_registration_path, class: "text-white font-bold hover:underline"%>
<% end %>
